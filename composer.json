{
    "name": "boxhead/soundcloud-sync",
    "description": "Sync your Soundcloud track data into Craft Entries",
    "type": "craft-plugin",
    "version": "2.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "soundcloud sync"
    ],
    "support": {
        "docs": "https://github.com/weareboxhead/soundcloud-sync/blob/master/README.md",
        "issues": "https://github.com/weareboxhead/soundcloud-sync/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Boxhead",
            "homepage": "https://boxhead.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "boxhead\\soundcloudsync\\": "src/"
        }
    },
    "extra": {
        "name": "Soundcloud Sync",
        "handle": "soundcloud-sync",
        "developer": "Boxhead",
        "developerUrl": "https://boxhead.io",
        "documentationUrl": "https://github.com/weareboxhead/soundcloud-sync/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/weareboxhead/soundcloud-sync/master/CHANGELOG.md",
        "components": {
            "soundcloudEntries": "boxhead\\soundcloudsync\\services\\SoundcloudEntries",
            "soundcloudCategories": "boxhead\\soundcloudsync\\services\\SoundcloudCategories"
        },
        "class": "boxhead\\soundcloudsync\\SoundcloudSync"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    }
}
